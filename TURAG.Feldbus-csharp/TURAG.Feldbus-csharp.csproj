<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>TURAG.Feldbus-csharp</AssemblyName>
    <RootNamespace>TURAG.Feldbus</RootNamespace>
    <Authors>Martin Oemus</Authors>
    <Version>0.2.0</Version>
    <RepositoryUrl>https://github.com/turag-ev/TURAG-Feldbus-csharp</RepositoryUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Company>TURAG e.V.</Company>
    <Copyright>2020-2021</Copyright>
    <PackageProjectUrl>https://github.com/turag-ev/TURAG-Feldbus</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Windows.Compatibility" Version="5.0.2" />
  </ItemGroup>

</Project>
